@using CentralService.Models;

@model  List<PartialPaymentModel>

<table class="table table-striped table-bordered" id="PartialPayment-list" style="width:100%;font-size: 1vw;">
    <thead>
        <tr style="line-height: 1vh;">
           
            <th>ประเภทของการชำระ</th>
            <th>จำนวน</th>
            <th>เลขบัตร/เลขเอกสารอ้างอิง</th>
            <th>หมายเหตุ</th>
            @*<th>แก้ไข/ลบ</th>*@
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.ToList())
        {
        <tr>
            <td>@item.PaymentTypeName</td>
            <td>@item.Money</td>
            <td>@item.PaymentNumberId</td>
            <td>@item.Remark</td>
            @*<td>
                <a href="#"><i class="fa fa-edit" title="Edit"></i></a> | <a href="#"><i class="fa fa-remove" title="Delete"></i></a>

            </td>*@
        </tr>
        }
        

    </tbody>
   
</table>
<script>
    $(document).ready(function () {
        CreateDtTable();
    });
</script>

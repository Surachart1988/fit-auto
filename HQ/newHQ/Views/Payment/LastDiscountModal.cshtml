
@{
    ViewBag.Title = "LastDiscountModal";
}

<!-- Modal -->
<div class="modal right fade" id="lastDiscountModal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document" style="height: 20vh;">
        <div class="modal-content">

            <div class="modal-header">
                <h4 class="modal-title">ส่วนลดท้ายบิล</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>

            </div>

            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text" id="basic-addon1">ส่วนลดท้ายบิล</span>
                    </div>
                    <input type="text" class="form-control money" id="txtLastDiscount" aria-describedby="basic-addon1">
                </div>
                <button type="button" id="btnLastDiscount" class="btn btn-success">ยืนยัน</button>
            </div>

        </div><!-- modal-content -->
    </div><!-- modal-dialog -->
</div>


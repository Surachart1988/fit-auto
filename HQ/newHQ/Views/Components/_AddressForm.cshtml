@model CentralService.Models.AddressModel

<div class="form-group row has-feedback">
    <label for="inputadd_tumbol" class="col-sm-2 col-form-label text-right">จังหวัด<font color="#FF0000">*</font></label>
    <div class="col-sm-10">
        @Html.DropDownListFor(model => model.add_provid, new SelectList(Model.ProvinceList, "prov_id", "prov_name")
        , Model._mode == "v" ?
        (object)new { @class = "form-control", autocomplete = "off", @disabled = "disabled" } :
        (object)new
             {
                @class = "form-control",
                autocomplete = "off",
                data_error = "กรุณาระบุ จังหวัด",
                data_equals = "0",
                data_equals_error = "กรุณาระบุ จังหวัด",
                required = "required"
             })
        @*@Html.TextBoxFor(model => model.add_province, ""
                   , Model.Mode == "v" ? (object)new { @maxlength = "255", @class = "form-control", autocomplete = "off", @disabled = "disabled" }
                   : (object)new { @maxlength = "255", @class = "form-control", autocomplete = "off", data_error = "กรุณาระบุ จังหวัด", required = "required" })
            @Html.ValidationMessageFor(model => model.add_province, "", new { @class = "text-danger" })*@
        <span class="glyphicon form-control-feedback" aria-hidden="true" style="text-align: left;"></span>
        <div class="help-block with-errors"></div>
    </div>
</div>

